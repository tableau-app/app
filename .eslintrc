{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "react-app"],
  "rules": {
    "no-console": "warn",
    "indent": [
      "error",
      2,
      { "SwitchCase": 1 }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}